[codespell]
skip = .git,.gitlab,cmake,3rdparty,AUTHORS.md
quiet-level = 2
# Words to be ignored, put one word per line and sort alphabetically
ignore-words-list =
 asai,
 hist,
 globaly,
 localy,
 skelton,
 synopsys,
 te,
 yse,
